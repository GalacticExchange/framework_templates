[program:enchilada]
command=java -jar /usr/lib/enchilada/enchilada.jar
autostart=true
autorestart=true
stderr_logfile=/var/log/enchilada.err.log
stdout_logfile=/var/log/enchilada.out.log

[program:ruby_enchilada]
command=/var/www/apps/enchilada/rails s -p 3000
autostart=true
autorestart=true
stderr_logfile=/var/log/ruby_enchilada.err.log
stdout_logfile=/var/log/ruby_enchilada.out.log

