            <value>from java.nio.charset import StandardCharsets
from org.apache.commons.io import IOUtils
from org.apache.nifi.processor.io import StreamCallback
from org.python.core.util import StringUtil
import json
import sys
import traceback

class Callback(StreamCallback):
    def __init__(self):
        pass

    def process(self, inputStream, outputStream):
        try:
            json_string = IOUtils.toString(inputStream, StandardCharsets.UTF_8)
            json_string = json.loads(json_string)['response']
            json_string = json_string.replace('\n', '')
            json_string = json_string.replace('\\', '')
            sentences = json.loads(json_string)['sentences']

            answer = 0
            amount = 0
            for sentence in sentences:
                print(sentence['sentimentValue'])
                answer += (int(sentence['sentimentValue']) - 2)
                amount += 1

            num_answer = float(answer) / float(amount)
            if num_answer &lt;= -1.5:
                string_answer = "Very negative"
            elif num_answer &lt;= -0.5:
                string_answer = "Negative"
            elif num_answer &lt; 0.5:
                string_answer = "Neutral"
            elif num_answer &lt; 1.5:
                string_answer = "Positive"
            else:
                string_answer = "Very positive"
            json_answer = {
                'doc':{}
            }
            json_answer['doc']['sentiment_str'] = string_answer
            json_answer['doc']['sentiment_num'] = round(num_answer, 2)
            outputStream.write(StringUtil.toBytes(json.dumps(json_answer, indent=4)))
        except:
            traceback.print_exc(file=open('/tmp/nifi_corenlp_flow_error.txt','w'))
            raise

flowFile = session.get()

if flowFile != None:
    flowFile = session.write(flowFile, Callback())
    session.transfer(flowFile, REL_SUCCESS)
    session.commit()</value>
